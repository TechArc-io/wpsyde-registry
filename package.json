{
  "name": "wpsyde-ui",
  "version": "1.0.4",
  "description": "Professional UI component manager for WordPress (shadcn/ui style)",
  "private": false,
  "scripts": {
    "verify": "node scripts/verify-registry.js",
    "test": "node scripts/verify-registry.js",
    "generate-keys": "./scripts/generate-keys.sh",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "prettier --check . && node scripts/verify-registry.js",
    "wpsyde": "node scripts/wpsyde-cli.js",
    "wpsyde:init": "node scripts/wpsyde-cli.js init",
    "wpsyde:list": "node scripts/wpsyde-cli.js list",
    "wpsyde:add": "node scripts/wpsyde-cli.js add",
    "build:registry": "node scripts/build-registry.js",
    "build:packaging": "node scripts/package-components.js",
    "serve:local": "node scripts/serve-local.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TechArc-io/wpsyde-registry.git"
  },
  "keywords": [
    "wpsyde",
    "wordpress",
    "components",
    "registry",
    "ui",
    "component-manager",
    "wordpress-themes",
    "shadcn-ui"
