  "author": "TechArc.io <asem@techarcio.io>",
  "license": "MIT",
  "homepage": "https://registry.wpsyde.com",
  "main": "scripts/wpsyde-cli.js",
  "bin": {
    "wpsyde-ui": "scripts/wpsyde-cli.js"
  },
  "dependencies": {
    "adm-zip": "^0.5.10"
  },
  "devDependencies": {
    "prettier": "^3.0.0"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
